<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.withered.mapper.AccountMapper">
    <select id="get" resultType="Account">
        select * from myweb.account
    </select>
    <select id="getAccountByName" resultType="Account">
        select * from myweb.account where name = #{name}
    </select>
</mapper>
